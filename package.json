{
  "name": "of-course-i-still-log-you",
  "displayName": "Of course I still log you",
  "description": "Amazing log helper",
  "version": "0.0.14",
  "engines": {
    "vscode": "^1.61.0"
  },
  "categories": [
    "Visualization"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./dist/local/extension.js",
  "browser": "./dist/web/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "icon": "resources/icon.svg",
          "id": "logyoucontainer",
          "title": "Of course I still log you"
        }
      ]
    },
    "views": {
      "logyoucontainer": [
        {
          "id": "logyouview",
          "name": "Of course I still log you",
          "icon": "resources/icon.svg",
          "contextualTitle": "Of course I still log you",
          "type": "tree",
          "visibility": "visible"
        }
      ]
    },
    "commands": [
      {
        "command": "logyouview.parseOutput",
        "title": "Follow, filter and colorize a command's output",
        "icon": "$(terminal)"
      },
      {
        "command": "logyouview.add",
        "title": "Add",
        "icon": "$(add)"
      },
      {
        "command": "logyouview.edit",
        "title": "Edit",
        "icon": "$(edit)"
      },
      {
        "command": "logyouview.toggleDisabled",
        "title": "Toggle",
        "icon": "$(star-half)"
      },
      {
        "command": "logyouview.openjson",
        "title": "Rules as JSON",
        "icon": "$(json)"
      },
      {
        "command": "logyouview.moveUp",
        "title": "Edit",
        "icon": "$(arrow-up)"
      },
      {
        "command": "logyouview.exportLines",
        "title": "Export lines",
        "icon": "$(export)"
      }
    ],
    "menus": {
      "view/title": [
        {
          "command": "logyouview.add",
          "when": "view == logyouview",
          "group": "navigation"
        },
        {
          "command": "logyouview.exportLines",
          "when": "view == logyouview",
          "group": "navigation"
        },
        {
          "command": "logyouview.openjson",
          "when": "view == logyouview",
          "group": "navigation"
        },
        {
          "command": "logyouview.parseOutput",
          "when": "view == logyouview",
          "group": "navigation"
        }
      ],
      "view/item/context": [
        {
          "command": "logyouview.edit",
          "when": "view == logyouview",
          "group": "inline"
        },
        {
          "command": "logyouview.toggleDisabled",
          "when": "view == logyouview",
          "group": "inline"
        },
        {
          "command": "logyouview.moveUp",
          "when": "view == logyouview",
          "group": "inline"
        }
      ]
    }
  },
  "scripts": {
    "test": "vscode-test-web --browserType=chromium --extensionDevelopmentPath=.",
    "pretest": "npm run compile-web",
    "vscode:prepublish": "npm run package-web",
		"compile": "tsc -p ./",
    "compile-web": "webpack",
    "watch-web": "webpack --watch",
    "package-web": "webpack --mode production --devtool hidden-source-map",
    "lint": "eslint src --ext ts",
    "run-in-browser": "vscode-test-web --browserType=chromium --extensionDevelopmentPath=. ."
  },
  "devDependencies": {
    "@types/mocha": "^9.0.0",
    "@types/vscode": "^1.61.0",
    "@types/webpack-env": "^1.16.2",
    "@typescript-eslint/eslint-plugin": "^4.31.1",
    "@typescript-eslint/parser": "^4.31.1",
    "@vscode/test-web": "^0.0.13",
    "assert": "^2.0.0",
    "eslint": "^7.32.0",
    "mocha": "^9.1.1",
    "process": "^0.11.10",
    "ts-loader": "^9.2.5",
    "typescript": "^4.4.3",
    "webpack": "^5.52.1",
    "webpack-cli": "^4.8.0",
    "@vscode/codicons": "^0.0.26",
    "copy-webpack-plugin": "^10.0.0"
  },
  "author": {
    "name": "Diego PÃ©rez",
    "email": "eazel7@gmail.com"
  },
  "publisher": "eazel7",
  "repository": {
    "url": "https://github.com/eazel7/of-course-i-still-log-you.git"
  },
  "dependencies": {
    "@vscode/codicons": "^0.0.26",
    "copy-webpack-plugin": "^10.0.0"
  },
  "icon": "extension-icon.png"
}
